[ignore]
.*/vendor/.*
.*/node_modules/fbjs/.*
[include]

[libs]
./interfaces/CSSModule.js
./interfaces/YAMLModule.js

[options]
suppress_comment= \\(.\\|\n\\)*\\$FlowIssue
module.name_mapper='.+\.s?css' -> 'CSSModule'
module.name_mapper='.+\.ya?ml' -> 'YAMLModule'
module.system=node
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.css
module.file_ext=.scss
module.file_ext=.yml
module.file_ext=.yaml
